<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jefriap.hometestcode">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HomeTestCodeFuree"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".ui.detail.DetailScreenActivity"
            android:exported="false"
            android:label="@string/title_activity_detail_screen"
            android:theme="@style/Theme.HomeTestCodeFuree" />
        <activity
            android:name=".ui.getstarted.GetStartedActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.HomeTestCodeFuree">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.main.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.HomeTestCodeFuree.NoActionBar" />
    </application>

</manifest>